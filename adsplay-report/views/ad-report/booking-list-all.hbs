<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            All Bookings
        </h1>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">
        <div class="table-responsive">
            <table class="table table-bordered table-hover table-striped">
                <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Total Impression</th>
                    <th style="display: none"><a href="javascript:" title="Complete-View rate">CVR</a></th>
                    <th>Booking Time</th>
                </tr>
                </thead>
                <tbody>

                {{#each bookings}}
                    <tr {{#if active }} class="active" {{/if}}>
                        <td>{{id}}</td>
                        <td><a href="/booking/list/all/#">{{name}}</a></td>
                        <!--<td><a href="/booking/{{id}}">{{name}}</a></td>-->
                        <td>{{formatCurrency imp}}</td>

                        <td>{{runDate}}</td>
                    </tr>
                {{/each}}

                </tbody>
            </table>
        </div>
    </div>
</div>


<script>
    $(function () {
        $(document).tooltip();
    });
</script>

